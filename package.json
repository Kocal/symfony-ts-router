{
  "name": "symfony-ts-router",
  "version": "1.0.1",
  "description": "Typescript port of javascript router from FOSJsRoutingBundle",
  "keywords": [
    "symfony",
    "typescript",
    "router",
    "fos",
    "ts",
    "FOSJsRoutingBundle",
    "fos js router"
  ],
  "main": "./dist/router.js",
  "types": "./dist/router.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/laland/symfony-ts-router.git"
  },
  "bugs": "https://github.com/laland/symfony-ts-router/issues",
  "homepage": "https://github.com/laland/symfony-ts-router#readme",
  "author": "Reznichenko Vladimir <laland@ya.ru>",
  "license": "MIT",
  "scripts": {
    "test": "jasmine-ts \"src/**/*.spec.ts\"",
